{
  "name": "hugo-fixit-yaml",
  "version": "1.0.0",
  "description": "Hugo + FixIt bundle",
  "private": "true",
  "scripts": {
    "test": "npm run",
    "create": "hugo nee content",
    "build": "hugo --gc --minify --logLevel info",
    "server": "hugo server -D -E -F -N -O --disableFastRender --renderToMemory --bind 0.0.0.0 -b http://localhost:1313",
    "server:production": "npm run server -- -e production",
    "update": "hugo mod get -u && hugo mod tidy && hugo mod graph"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/attajak/hugo-fixit-yaml.git"
  },
  "keywords": [
    "hugo",
    "fixit",
    "bundle",
    "attajak"
  ],
  "author": "attajak (https://github.com/attajak/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/attajak/hugo-fixit-yaml/issues"
  },
  "homepage": "https://github.com/attajak/hugo-fixit-yaml#readme"
}
